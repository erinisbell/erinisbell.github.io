<!doctype html>
<html>
<head>

  <meta charset="utf-8" />

  <!-- Always force latest IE rendering engine or request Chrome Frame -->
  <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">

  <!-- Use title if it's in the page YAML frontmatter -->
  <title><%= current_page.data.title || "Erin's Blog" %></title>

  <%= stylesheet_link_tag  "all" %>
  <%= javascript_include_tag  "all" %>
</head>

<body class="<%= page_classes %>">
  <nav class="homenav">
    <img src="./images/EI-Black.png">
    <a class="btn contact" href="mailto:erinmarie.isbell@gmail.com">Contact</a>
    <%= link_to "About Me", "/about.html", class: 'btn' %>
    <%= link_to "Home", "/", class: "btn" %>
    <hr>
  </nav>
  <div class="container">
    <%= yield %>
  </div>
  <hr>
</body>
</html>
